CREATE TABLE pdbbind.binding_INDEX (
	id integer generated by default as identity PRIMARY KEY,
	pdb_code char(4) REFERENCES pdbbind.binding (pdb_code) ON delete restrict UNIQUE NOT NULL,
	resolution character varying,
	release_year char(4),
	pLog numeric NOT NULL,
	affinity_data character varying NOT NULL,
	reference character varying NOT NULL,
	ligand_name character varying NOT NULL
)
